#include <bits/stdc++.h>
using namespace std;

int main()
{
    cin.tie(NULL);
    cout.tie(NULL);
    ios_base::sync_with_stdio(false);

    //freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);

    int a, b;
    cin >> a >> b;
    int x = 0, y = 0;
    while(a)
    {
        x += a % 10;
        a /= 10;
    }
    while(b)
    {
        y += b % 10;
        b /= 10;
    }
    cout << max(x, y);
}


